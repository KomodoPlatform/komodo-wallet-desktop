    "entt",
    "boost-multiprecision",
    "boost-filesystem",
    "boost-thread",
    "boost-random",
    "boost-lockfree",
    "boost-stacktrace",
    "doctest",
    "fmt",
    "nlohmann-json",
    "range-v3",
    "libsodium",
    "spdlog",
    {
      "name": "spdlog",
      "features": [
        "wchar",
        "wchar-filenames"
